			[OBJECT:BlueprintPin]
                [PARAMLITERAL:type]
                    integer
                [/PARAMLITERAL]
                [PARAMLITERAL:id]
                    Return_Value
                [/PARAMLITERAL]
                [PARAMLITERAL:title]
